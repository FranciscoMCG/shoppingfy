ge({itemId: item.SKU, value: parseInt(e.target.value) * item.price}))\n    const handleClick = () => dispatch(reset({itemId: item.SKU}))\n    const handleBlur = (e: any) => console.log('d')\n\n    const subTotal = parseFloat(basket?.[item.SKU]) || 0\n\n  return (\n    <ShoppingItemWrapper>\n    <Name>{item.name}</Name>\n      <Input type=\"number\" id=\"qty\" name=\"qty\" min=\"0\" max=\"3\" placeholder=\"0\" onChange={handleChange} onBlur={handleBlur} />\n      <SubTotal>{item.currency}{subTotal.toFixed(2)}</SubTotal>\n      <Delete onClick={handleClick}>X</Delete>\n    </ShoppingItemWrapper>\n  )\n}\n\nexport default ShoppingItem\n","/Users/francisco.gomes1/Workspace/apps/projects/shoppingfy/src/views/ShoppingBasket/index.ts",[],{"ruleId":"52","replacedBy":"53"},{"ruleId":"54","replacedBy":"55"},{"ruleId":"56","severity":1,"message":"57","line":1,"column":21,"nodeType":"58","messageId":"59","endLine":1,"endColumn":29},{"ruleId":"56","severity":1,"message":"60","line":2,"column":22,"nodeType":"58","messageId":"59","endLine":2,"endColumn":31},{"ruleId":"56","severity":1,"message":"61","line":2,"column":43,"nodeType":"58","messageId":"59","endLine":2,"endColumn":53},{"ruleId":"56","severity":1,"message":"62","line":46,"column":10,"nodeType":"58","messageId":"59","endLine":46,"endColumn":15},{"ruleId":"56","severity":1,"message":"63","line":46,"column":17,"nodeType":"58","messageId":"59","endLine":46,"endColumn":25},"no-native-reassign",["64"],"no-negated-in-lhs",["65"],"@typescript-eslint/no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","'FormEvent' is defined but never used.","'FocusEvent' is defined but never used.","'error' is assigned a value but never used.","'setError' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]